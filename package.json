{
  "name": "@icp-sdk/auth",
  "version": "4.2.0",
  "author": "DFINITY Stiftung <sdk@dfinity.org>",
  "license": "Apache-2.0",
  "description": "Authentication library for Internet Computer web apps",
  "homepage": "https://js.icp.build/auth/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfinity/icp-js-auth.git"
  },
  "bugs": {
    "url": "https://github.com/dfinity/icp-js-auth/issues"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./client": {
      "types": "./dist/esm/client/index.d.ts",
      "import": "./dist/esm/client/index.js",
      "default": "./dist/esm/client/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "internet computer",
    "icp",
    "authentication",
    "internet identity"
  ],
  "scripts": {
    "build": "vite build && pnpm lint:package",
    "test": "vitest",
    "codestyle:check": "biome check",
    "codestyle:fix": "biome check --write",
    "lint:package": "publint --strict"
  },
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@icp-sdk/core": "5.0.0-beta.0",
    "@tanstack/config": "^0.22.1",
    "fake-indexeddb": "^6.2.5",
    "jsdom": "^27.2.0",
    "publint": "^0.3.15",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "4.0.14"
  },
  "peerDependencies": {
    "@icp-sdk/core": "5.0.0-beta.0"
  },
  "dependencies": {
    "idb": "^7.1.1"
  }
}
